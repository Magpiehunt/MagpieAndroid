<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <gradient
                android:startColor="@color/colorMagpieGreen"
                android:endColor="@color/colorMagpieBlue"
                android:angle="135" />

            <stroke
                android:width="2dp"
                android:color="#ff207d94" />
            <corners android:radius="33dp" />
        </shape>
    </item>
    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorMagpieWhite" />
            <corners android:radius="33dp" />
        </shape>
    </item>

</layer-list>